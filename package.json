{
  "name": "gh-forecast",
  "version": "1.1.0",
  "description": "Forecast your GHES Actions Minutes Usage - A GitHub CLI extension",
  "main": "dist/index.js",
  "bin": {
    "gh-forecast": "./bin/gh-forecast"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "ncc build src/index.ts -o dist --minify",
    "build:tsc": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "gh-extension",
    "github",
    "github-actions",
    "actions",
    "forecast",
    "billing",
    "ghes",
    "enterprise",
    "cost",
    "minutes",
    "cli"
  ],
  "author": "Austen Stone",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austenstone/gh-ghes-actions-forecast.git"
  },
  "bugs": {
    "url": "https://github.com/austenstone/gh-ghes-actions-forecast/issues"
  },
  "homepage": "https://github.com/austenstone/gh-ghes-actions-forecast#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@octokit/core": "^7.0.6",
    "@octokit/plugin-paginate-rest": "^14.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^17.0.0",
    "@octokit/plugin-retry": "^8.0.3",
    "@octokit/plugin-throttling": "^11.0.3",
    "@octokit/rest": "^21.0.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "date-fns": "^3.6.0",
    "p-limit": "^3.1.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0"
  }
